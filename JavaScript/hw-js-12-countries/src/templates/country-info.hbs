<h2 class="country-name">{{name}}</h2>
<div class="country-info">
    <div class="wrapper-info">
        <p class="country-info__item">Capital: {{capital}}</p>
        <p class="country-info__item">Population: {{population}}</p>
        <p class="country-info__item">languages</p>
        <ul class="languages-list">
            {{#each languages}}
            <li class="languages__list">{{name}}</li>
            {{/each}}
        </ul>
    </div>
    <img class="country-flag" src="{{flag}}" alt="flag of {{name}}">
</div>